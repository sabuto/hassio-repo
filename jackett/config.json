{
  "name": "Jackett",
  "version": "0.1.1",
  "slug": "hassio_jackett",
  "description" : "An addon to add jackett to hassio",
  "url" : "https://github.com/Sabuto/hassio-jackett",
  "arch": ["armv7", "aarch64", "amd64"],
  "startup": "application",
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:9118]",
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "apparmor": false,
  "ingress": true,
  "options": {
    "black_hole": "/share/blackhole"
  },
  "schema": {
    "black_hole": "str"
  },
  "ports" : {
    "9117/tcp": null
  },
  "ports_description" : {
    "9117/tcp": "Port for the web interface (not needed with ingress)"
  },
  "image" : "sabuto/{arch}-hassio-jackett"
}
